{
    "name": "itsimiro/laravel-openai",
    "description": "A laravel package to integrate openai in laravel application",
    "keywords": [
        "itsimiro",
        "laravel-openai",
        "openai",
        "open-ai",
        "laravel"
    ],
    "homepage": "https://github.com/itsimiro/laravel-openai",
    "license": "MIT",
    "authors": [
        {
            "name": "itsimiro",
            "email": "itsimiro@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^9.46.0|^10.1.5",
        "spatie/laravel-data": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Itsimiro\\OpenAI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itsimiro\\OpenAI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itsimiro\\OpenAI\\Providers\\OpenaiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
